import java.util.*;
    
    public class Main{
    
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int num1=sc.nextInt();
      int num2=sc.nextInt();
      int onum1=num1;
      int onum2=num2;
      int rem=0;
      while(num1%num2!=0)
      {
        rem=num1%num2;
        num1=num2;
        num2=rem;
      }
      int lcm=(onum1*onum2)/rem;
      System.out.println(rem);
      System.out.println(lcm);
     }
    }
