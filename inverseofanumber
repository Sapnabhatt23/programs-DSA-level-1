import java.util.*;

public class Main{

public static void main(String[] args) {
   Scanner sc=new Scanner(System.in);
   int n=sc.nextInt();
    int i=0,op=1;
    while(n!=0)
    {
        int od=n%10;
        int ip=od;
        int id=op;
        i=i+id*(int)Math.pow(10,ip-1);
        op++;
        n=n/10;
    }
    System.out.println(i);
 }
}
